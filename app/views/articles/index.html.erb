
<div class="container" style="margin-top: 70px;">

  <div class="row">
    <div class="col-sm-8 col-sm-offset-2" style="    padding-left: 200px;">
      <% @articles.each do |article| %>
        <h2 style="font-family: 'Roboto Slab', serif;"><%= link_to article.title, article_path(article) %></h2>
        <p><%= article.content[0..100] %> ...</p>
      <% end %>
    </div>
  </div>

</div>
